<!DOCTYPE html>
<html lang="pl">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Skontaktuj się z Forest Group</title>
    <!-- @include "head.kit" -->
    <link rel="stylesheet" href="./dist/css/contact.min.css">
</head>

<body>
    <!-- @include "nav.kit" -->
    <div class="contact">
        <div class="wrapper">
            <form action="" class="contact__form">
                <div class="contact__form-box">
                    <label for="" class="contact__form-label">Imię</label>
                    <input type="text" class="contact__form-input" placeholder="Podaj swoję imię">
                    <p class="contact__error">Lorem ipsum dolor sit amet.</p>
                </div>
                <div class="contact__form-box">
                    <label for="" class="contact__form-label">Nazwisko</label>
                    <input type="text" class="contact__form-input">
                    <p class="contact__error">Lorem ipsum dolor sit amet.</p>
                </div>
                <div class="contact__form-box">
                    <label for="" class="contact__form-label">E-mail</label>
                    <input type="text" class="contact__form-input">
                    <p class="contact__error">Lorem ipsum dolor sit amet.</p>
                </div>
                <div class="contact__form-box">
                    <label for="" class="contact__form-label">Wiadomość</label>
                    <textarea name="" id="" cols="30" rows="10" class="contact__form-textarea"></textarea>
                    <p class="contact__error">Lorem ipsum dolor sit amet.</p>
                </div>

            </form>
            <iframe class="contact__map"
                src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d2559.8980500889747!2d19.890269276854703!3d50.088195613380265!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x47165a326d015c3b%3A0x47a12cd367a1f34e!2sRestauracja%20McDonald&#39;s!5e0!3m2!1spl!2spl!4v1689879531505!5m2!1spl!2spl"
                allowfullscreen="" loading="lazy" referrerpolicy="no-referrer-when-downgrade"></iframe>

        </div>
    </div>

    <!-- @include "footer.kit" -->

    <script src="./dist/js/main.min.js"></script>
</body>

</html>